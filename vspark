#!/usr/bin/perl

use strict;
use warnings;

sub show {
    my $num = shift;
    my $max = shift;

    my @graph = qw{ ▏ ▎ ▍ ▌ ▋ ▊ ▉ █};
    my $index = ( $num * ( scalar(@graph) - 1 ) ) / $max;

    return $graph[$index];
}

chomp( @ARGV = <STDIN> ) unless @ARGV;

my @list = sort { $a <=> $b } @ARGV;

for my $i ( @ARGV ) {
    printf( "%s\n", show($i, $list[-1]) );
}
